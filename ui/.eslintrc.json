{
  "env": {
  "browser": true,
  "node": false
  },
  "extends": ["airbnb"],
  "rules": {
    "semi": [1, "always"],
    "react/jsx-filename-extension": [
      1,
      {
        "extensions": [".js", ".jsx"]
      }
    ],
    "linebreak-style": ["error", "windows"],
    "import/prefer-default-export": "off"
  }
}
